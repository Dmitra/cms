{
  "name": "@graphiy/server",
  "version": "0.1.0",
  "description": "Graphiy backend",
  "keywords": [
    "graphiy backend",
    "dynamic graph",
    "knowledge management",
    "network data"
  ],
  "homepage": "",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Graphiy",
  "repository": {
    "type": "git",
    "url": "https://github.com/Graphiy/graphiy"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=DEV yarn start",
    "doc": "jsdoc src -r -d docs",
    "lint": "eslint .",
    "start": "babel-node index.js",
    "pretest": "yarn lint",
    "test": "jest test"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ],
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": "@graphiy/eslint-config"
  },
  "dependencies": {
    "@graphiy/collection": "^0.1.0",
    "@graphiy/graph": "^0.1.0",
    "@graphiy/util": "^0.1.0",
    "@graphiy/provider-raw": "^0.1.0",
    "@graphiy/uuid62": "^0.1.0",
    "body-parser": "^1.17.2",
    "chalk": "^2.0.1",
    "eventemitter3": "^3.1.0",
    "express": "^4.15.3",
    "fs-extra": "^4.0.1",
    "lodash": "^4.17.10",
    "minimist": "^1.2.0",
    "moment": "^2.18.1",
    "multer": "^1.3.0",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "@graphiy/eslint-config": "^0.1.0",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.4",
    "jest": "^23.1.0"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.js"
    ],
    "transformIgnorePatterns": ["<rootDir>/node_modules/(?!@graphiy)"]
  }
}
